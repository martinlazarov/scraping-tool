<header>
    <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-primary p-0">
      <div class="container-fluid">
        <a class="navbar-brand" href="/">Azbouki Software Ltd.</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
          <ul class="navbar-nav ms-auto mb-2 mb-md-0">
            <% cfg.MENU.forEach(menuItem => { %>
            <li class="nav-item">
              <a class="nav-link<%= menuItem.active ? ' active': ''%>" 
                <% if (menuItem.active) { %>
                  aria-current="page"
                <% } %> 
                href="<%= menuItem.url %>"><%= menuItem.name %></a>
            </li>
            <% }); %>
            <% if (cfg.loggedIn) { %>
            <li class="nav-item">
              <a class="nav-link" id="logout">Изход</a>
            </li>
            <% } %>
          </ul>
        </div>
      </div>
    </nav>
</header>